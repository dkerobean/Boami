{
  "name": "BOAMI",
  "version": "6.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "migrate": "tsx src/lib/database/migrations/run-migrations.ts",
    "migrate:up": "tsx src/lib/database/migrations/run-migrations.ts up",
    "migrate:down": "tsx src/lib/database/migrations/run-migrations.ts down",
    "db:migrate": "tsx src/lib/database/migrate.ts up",
    "db:migrate:subscription": "tsx src/lib/database/migrate.ts up",
    "db:rollback": "tsx src/lib/database/migrate.ts down",
    "db:status": "tsx src/lib/database/migrate.ts status",
    "db:verify": "tsx src/lib/database/migrate.ts verify",
    "db:verify:migrations": "tsx src/lib/database/migrate.ts verify",
    "db:check": "tsx -e \"require('./src/lib/database/mongoose-connection').connectToDatabase().then(() => { console.log('Database connection successful'); process.exit(0); }).catch((err) => { console.error('Database connection failed:', err); process.exit(1); })\"",
    "db:backup": "node scripts/backup-database.js",
    "db:restore": "node scripts/restore-database.js",
    "deploy:production": "bash scripts/deploy-production.sh",
    "deploy:staging": "bash scripts/deploy-production.sh staging",
    "ssl:setup": "sudo bash scripts/ssl-setup.sh",
    "health:check": "curl -f http://localhost:3000/api/health || curl -f https://localhost:3000/api/health",
    "test:integration": "jest --testPathPattern=integration",
    "test:integration:subscription": "jest --testPathPattern=subscription.*integration",
    "test:integration:payment": "jest --testPathPattern=payment.*integration",
    "test:webhook": "jest --testPathPattern=webhook",
    "test:e2e": "jest --testPathPattern=e2e",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:reload": "pm2 reload ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:monit": "pm2 monit",
    "logs:subscription": "tail -f /var/log/subscription-system/combined.log",
    "logs:error": "tail -f /var/log/subscription-system/error.log",
    "monitoring:health": "curl -H 'Authorization: Bearer $CRON_SECRET' http://localhost:3000/api/cron/system-health-check",
    "data:migrate-users": "node scripts/subscription-data-utils.js migrate-users",
    "data:migrate-legacy": "node scripts/subscription-data-utils.js migrate-legacy",
    "data:cleanup-expired": "node scripts/subscription-data-utils.js cleanup-expired",
    "data:cleanup-transactions": "node scripts/subscription-data-utils.js cleanup-transactions",
    "data:export": "node scripts/subscription-data-utils.js export",
    "data:backup": "node scripts/subscription-data-utils.js backup",
    "data:restore": "node scripts/subscription-data-utils.js restore",
    "test:production": "tsx src/__tests__/run-production-tests.ts",
    "validate:deployment": "bash scripts/validate-production-deployment.sh",
    "validate:production": "bash scripts/validate-production-deployment.sh your-domain.com https"
